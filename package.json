{
  "name": "@jlongyam/cmd",
  "version": "1.2.2",
  "description": "JS Command utility",
  "keywords": [
    "args",
    "utility"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/cmd.git"
  },
  "homepage": "https://github.com/jlongyam/cmd",
  "main": "./dist/cmd.cjs",
  "scripts": {
    "build:flags": "bun build --format=esm --outfile=src/flags.mjs src/flags.cjs",
    "build:cmd": "bun build --production --format=esm --outfile=dist/cmd.mjs src/cmd.mjs",
    "build:cmd:cjs": "rollup -c",
    "test": "bun test",
    "types": "tsc"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "es-check": "^9.5.4",
    "flaget": "^2.1.1",
    "rollup": "^4.57.1",
    "typescript": "^5.9.3"
  },
  "exports": {
    "import": "./dist/cmd.mjs",
    "require": "./dist/cmd.cjs",
    "types": "./dist/cmd.d.mts"
  },
  "files": [
    "dist",
    "test"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  }
}
