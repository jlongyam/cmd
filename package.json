{
  "name": "@jlongyam/cmd",
  "version": "1.1.3",
  "description": "JS Command utility",
  "keywords": [
    "args",
    "utility"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/cmd.git"
  },
  "homepage": "https://github.com/jlongyam/cmd",
  "main": "./dist/cmd.mjs",
  "scripts": {
    "build:flags": "bun build --production --format=esm --outfile=src/flags.js src/flags.cjs",
    "build:cmd": "bun build --production --format=esm --outfile=dist/cmd.mjs src/cmd.js",
    "build:cmd:cjs": "bun build --production --format=cjs --outfile=dist/cmd.cjs src/cmd.js",
    "test": "bun test",
    "types": "tsc"
  },
  "devDependencies": {
    "flaget": "^2.1.1",
    "typescript": "^5.9.3"
  },
  "exports": {
    "import": "./dist/cmd.mjs",
    "require": "./dist/cmd.cjs",
    "types": "./dist/cmd.d.mts"
  },
  "files": [
    "dist",
    "test"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  }
}
